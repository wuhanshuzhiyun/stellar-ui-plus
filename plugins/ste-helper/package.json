        "package": "vsce package",
        "login": "vsce login StellarUI",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/node": "20.x",
        "@types/vscode": "^1.94.0",
        "@vscode/vsce": "^3.2.0",
        "typescript": "^5.6.2"
    }
