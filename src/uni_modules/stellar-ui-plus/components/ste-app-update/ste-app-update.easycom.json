{
    "name": "ste-app-update",
    "description": "按钮组件",
    "example": "<ste-app-update></ste-app-update>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=app-update",
    "attributes": [
        {
            "name": "clientId",
            "description": "应用编码",
            "type": "string",
            "default": ""
        },
        {
            "name": "clientSecret",
            "description": "应用密钥",
            "type": "string",
            "default": ""
        },
        {
            "name": "apiUrl",
            "description": "更新API地址",
            "type": "string",
            "default": "https://zboa.whzb.com/inte-cloud-dev/blade-system/api/inte/client/ver/currentDetail"
        },
        {
            "name": "[event]cancel",
            "description": "取消更新",
            "type": "() => void"
        },
        {
            "name": "[event]complete",
            "description": "取消，成功更新都会执行",
            "type": "() => void"
        }
    ]
}
