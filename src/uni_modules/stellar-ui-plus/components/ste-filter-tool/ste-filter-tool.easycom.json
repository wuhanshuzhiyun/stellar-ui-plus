{
    "name": "ste-filter-tool",
    "description": "筛选选项",
    "example": "<ste-filter-tool></ste-filter-tool>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=filter-tool",
    "attributes": [
        {
            "name": "value",
            "description": "当前值，支持v-model:value双向绑定",
            "type": "object[]",
            "default": ""
        },
        {
            "name": "activeColor",
            "description": "激活状态颜色",
            "type": "string",
            "default": "#0275FF"
        },
        {
            "name": "inactiveColor",
            "description": "非激活状态颜色",
            "type": "string",
            "default": "#555A61"
        },
        {
            "name": "menuData",
            "description": "菜单数据列表",
            "type": "FilterItem[]",
            "default": "[]"
        },
        {
            "name": "[event]reset",
            "description": "点击重置时触发",
            "type": "() => void",
            "params": []
        },
        {
            "name": "[event]confirm",
            "description": "菜单变化时触发",
            "type": "(items: FilterItem[]) => void",
            "params": [
                {
                    "name": "items",
                    "description": "变化后的菜单项列表"
                }
            ]
        }
    ]
}
