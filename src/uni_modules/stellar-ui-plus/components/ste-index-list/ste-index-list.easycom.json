{
    "name": "ste-index-list",
    "description": "IndexList 索引列表",
    "example": "<ste-index-list><ste-index-item>节点1</ste-index-item></ste-index-list>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/?projectName=stellar-ui-plus&menu=%E7%BB%84%E4%BB%B6&active=ste-index-list",
    "attributes": [
        {
            "name": "active",
            "description": "当前激活的节点",
            "type": "number",
            "default": 0
        },
        {
            "name": "height",
            "description": "滚动区域高度",
            "type": "number | string",
            "default": "100%"
        },
        {
            "name": "sticky",
            "description": "默认标题是否粘性布局",
            "type": "boolean",
            "default": "true"
        },
        {
            "name": "inactiveColor",
            "description": "右边锚点状态非激活时的颜色",
            "type": "string",
            "default": "#666"
        },
        {
            "name": "activeColor",
            "description": "右边锚点状态激活时的颜色",
            "type": "string",
            "default": "#0090FF"
        },
        {
            "name": "[event]change",
            "description": "激活节点改变时触发",
            "type": "(index:number)=>void"
        },
        {
            "name": "[event]clickItem",
            "description": "点击右边锚点时触发",
            "type": "(title:string,text:string)=>void"
        }
    ]
}
