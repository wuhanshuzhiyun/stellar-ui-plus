{
    "name": "ste-main-info",
    "description": "主信息展示",
    "example": "<ste-main-info></ste-main-info>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=main-info",
    "attributes": [
        {
            "name": "avatar",
            "description": "头像url",
            "type": "string",
            "default": ""
        },
        {
            "name": "nickname",
            "description": "昵称",
            "type": "string",
            "default": ""
        },
        {
            "name": "loginStatus",
            "description": "登录状态 0未登录 1已登录",
            "type": "number",
            "default": 0
        },
        {
            "name": "showTitleIcon",
            "description": "是否显示主标题后面的图标",
            "type": "boolean",
            "default": false
        },
        {
            "name": "fontFamily",
            "description": "icon的字体名",
            "type": "string",
            "default": ""
        },
        {
            "name": "loginStatus",
            "description": "登录状态",
            "type": "Number",
            "default": 1,
            "values": [
                {
                    "name": 0,
                    "description": "未登录"
                },
                {
                    "name": 1,
                    "description": "登录"
                }
            ]
        },
        {
            "name": "loginSrc",
            "description": "未登录头像",
            "type": "string",
            "default": ""
        },
        {
            "name": "loginTitle",
            "description": "未登录标题",
            "type": "string",
            "default": ""
        },
        {
            "name": "[event]avatar-click",
            "description": "头像点击时触发",
            "type": "(]) => void",
            "params": []
        },
        {
            "name": "[event]user-click",
            "description": "用户信息点击时触发",
            "type": "(]) => void",
            "params": []
        },
        {
            "name": "[event]loginTitleClick",
            "description": "未登录文本点击时触发",
            "type": "(]) => void",
            "params": []
        }
    ]
}
