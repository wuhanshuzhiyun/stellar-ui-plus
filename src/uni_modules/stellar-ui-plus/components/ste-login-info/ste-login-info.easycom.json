{
    "name": "ste-main-info",
    "description": "主信息展示",
    "example": "<ste-main-info></ste-main-info>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=main-info",
    "attributes": [
        {
            "name": "mainColor",
            "description": "主颜色",
            "type": "string",
            "default": ""
        },
        {
            "name": "showAvatar",
            "description": "是否显示头像，若未配置头像url也不显示头像",
            "type": "boolean",
            "default": "true"
        },
        {
            "name": "avatarUrl",
            "description": "头像url",
            "type": "string",
            "default": ""
        },
        {
            "name": "title",
            "description": "主标题",
            "type": "string",
            "default": ""
        },
        {
            "name": "subTitle",
            "description": "副标题",
            "type": "string",
            "default": ""
        },
        {
            "name": "subTitleIcon",
            "description": "副标题图标code",
            "type": "string",
            "default": ""
        },
        {
            "name": "fontFamily",
            "description": "icon的字体名",
            "type": "string",
            "default": "ste-iconfont-1709689042473"
        },
        {
            "name": "loginStatus",
            "description": "登录状态",
            "type": "Number",
            "default": 1,
            "values": [
                {
                    "name": 0,
                    "description": "未登录"
                },
                {
                    "name": 1,
                    "description": "登录"
                }
            ]
        },
        {
            "name": "loginSrc",
            "description": "未登录头像",
            "type": "string",
            "default": ""
        },
        {
            "name": "loginTitle",
            "description": "未登录标题",
            "type": "string",
            "default": ""
        },
        {
            "name": "[event]avatar-click",
            "description": "头像点击时触发",
            "type": "(]) => void",
            "params": []
        },
        {
            "name": "[event]user-click",
            "description": "用户信息点击时触发",
            "type": "(]) => void",
            "params": []
        },
        {
            "name": "[event]loginTitleClick",
            "description": "未登录文本点击时触发",
            "type": "(]) => void",
            "params": []
        }
    ]
}
