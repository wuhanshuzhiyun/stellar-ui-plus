                {
                    "name": "ensp",
                    "description": "中文字符空格一半大小"
                },
                {
                    "name": "emsp",
                    "description": "中文字符空格大小"
                },
                {
                    "name": "nbsp",
                    "description": "根据字体设置的空格大小"
                }
            ]
        },
        {
            "name": "userSelect",
            "description": "文本是否可选",
            "type": "boolean",
            "default": false
        }
    ]
}
