{
    "name": "ste-rich-text",
    "description": "阅读更多",
    "example": "<ste-rich-text>文本文本文本文本文本文本文本文本文本</ste-rich-text>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=rich-text",
    "attributes": [
        {
            "name": "text",
            "description": "富文本值",
