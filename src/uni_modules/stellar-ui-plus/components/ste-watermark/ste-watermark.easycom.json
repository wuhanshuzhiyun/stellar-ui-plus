{
    "name": "ste-watermark",
    "description": "水印组件",
    "example": "<ste-watermark></ste-watermark>",
    "tutorial": "https://stellar-ui.intecloud.com.cn/plus/#/?active=watermark",
    "attributes": [
        {
            "name": "width",
            "description": "水印的宽度， content 的默认值是它自己的宽度",
            "type": "number",
            "default": 120
        },
        {
            "name": "height",
            "description": "水印的高度， content 的默认值是它自己的高度",
            "type": "number",
            "default": "64"
        },
        {
            "name": "rotate",
            "description": "水印的旋转角度, 单位 °",
            "type": "number",
            "default": "-22"
        },
        {
            "name": "zIndex",
            "description": "水印元素的 z-index 值",
            "type": "number",
            "default": "9"
        },
        {
            "name": "image",
            "description": "水印图片，建议使用 2x 或 3x 图像",
            "type": "string"
        },
        {
            "name": "content",
            "description": "水印文本内容",
            "type": "string / object"
        },
        {
            "name": "font",
            "description": "文字样式",
            "type": "Font"
        },
        {
            "name": "gap",
            "description": "水印之间的间距",
            "type": "object",
            "default": [50, 50]
        },
        {
            "name": "offset",
            "description": "水印从容器左上角的偏移 默认值为 gap/2",
            "type": "object",
            "default": "[gap[0]/2, gap[1]/2]"
        }
    ]
}
